# MediTrack Configuration
# Copy this file to .env and fill in your values

# OpenSky Network API Credentials (optional but recommended)
# Register at https://opensky-network.org/accounts/login
# Authenticated users have better rate limits and access to historical data
OPENSKY_USERNAME=your_username_here
OPENSKY_PASSWORD=your_password_here

# Rate Limiting Settings
# Adjust based on your OpenSky account type
OPENSKY_RATE_LIMIT_CALLS=10
OPENSKY_RATE_LIMIT_PERIOD=1.0

# Cache Settings
CACHE_ENABLED=true
CACHE_MAX_AGE_SECONDS=60

# Filtering Settings
# Set to true to exclude aircraft registered to individuals
EXCLUDE_INDIVIDUAL_OWNERS=false

# Minimum confidence level for filtering ('low', 'medium', 'high')
MIN_CONFIDENCE_LEVEL=low

# Regional Tracking
# Filter tracking to specific US region: 'northeast', 'midwest', 'south', 'west', or 'all'
# Leave empty or set to 'all' to track all US aircraft
# If not set, you will be prompted interactively
TRACKING_REGION=

# Logging
LOG_LEVEL=INFO
